#!/usr/bin/expect -f

# Spawn the cai process
spawn cai

# Wait for the CAI> prompt
expect "CAI>"

# Send the command
send "/mcp load http://mcp-server:8085/sse metasploit\n"

# Wait for the CAI> prompt
expect "CAI>"

# Send the command to set the CAI server
send "/mcp set http://mcp-server:8084/sse zap-proxy\n"

# Wait for the CAI> prompt
expect "CAI>"

# Interact with the user
interact